/**
 * COCHINTU FLORIN
 */

package Cochintu_RMI;

import java.rmi.Remote;
import java.rmi.RemoteException;

///Se declara metodele invocate la distanta carepot fi invocate prin intermediul interfetei
public interface CalculatorInterface extends Remote{
    public int add(int x, int y)throws RemoteException;
    public int sub(int x, int y)throws RemoteException;
    public int mul(int x, int y)throws RemoteException;
    public int div(int x, int y)throws RemoteException;
    public int mod(int x, int y)throws RemoteException;
    public int op1(int x, int y)throws RemoteException;
    public int op2(int x, int y)throws RemoteException;
    public int op3(int x, int y)throws RemoteException;
}
